@page
@model Disable2faModel
@{
    ViewData["Title"] = "Onemogući two-factor autentifikaciju (2FA)";
    ViewData["ActivePage"] = ManageNavPages.TwoFactorAuthentication;
}

<partial name="_StatusMessage" for="StatusMessage" />
<h2 class="text-white">@ViewData["Title"]</h2>

<div class="alert alert-warning" role="alert">
    <p>
        <strong class="text-white">>Ovim onemogućavate 2FA autentifikaciju.</strong>
    </p>
</div>

<div>
    <form method="post" class="form-group">
        <button class="btn btn-danger" type="submit">Onemogući 2FA</button>
    </form>
</div>
